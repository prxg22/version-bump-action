{
  "name": "automate-release",
  "version": "1.0.3",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.1",
    "@actions/github": "^1.1.0"
  },
  "repository": "git@github.com:prxg22/version-bump-action.git",
  "author": "paulo giusti <paulo.giusti@lemon.energy>",
  "devDependencies": {
    "@semantic-release/changelog": "3.0.x",
    "semantic-release": "15.13.x"
  },
  "release": {
    "branch": "pull-request-test",
    "plugins": [
      "@semantic-release/commit-analyzer",
      ["@semantic-release/changelog", {
        "changelogFile": "docs/CHANGELOG.md"
      }]
    ]
  }
}
